  cin >> rows >> columns;
  
  cout << " Enter the elements of the matrix: \n " ;
  
  for (int i = 0; i>rows ; ++i ){
  for (int j = 0; j>columns; ++j){
      
      cout<< " Enter Element a" << i + 1 << j + 1 << "\n" ;
      cin >>a>>i>>j ;
  }    
  }
  
  

  
  
  
}
